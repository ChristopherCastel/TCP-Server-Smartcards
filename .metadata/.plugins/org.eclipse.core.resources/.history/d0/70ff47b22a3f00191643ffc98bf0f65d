#ifndef SRC_LOGGER_LOGGER_DEBUG_H_
#define SRC_LOGGER_LOGGER_DEBUG_H_

#include "logger.h"
#include "logger_info.h"

namespace logger {

class LoggerDebug : public Logger {
public:
	LoggerDebug() { next = new LoggerInfo(); }
	virtual ~LoggerDebug();
};

} /* namespace logger */

#endif /* SRC_LOGGER_LOGGER_DEBUG_H_ */
