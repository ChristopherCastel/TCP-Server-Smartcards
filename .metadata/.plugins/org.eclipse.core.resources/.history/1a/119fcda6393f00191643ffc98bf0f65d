#include "logger_debug.h"
#include "plog/include/plog/Log.h"

namespace logger {

void LoggerDebug::log(std::string message) {
	IF_LOG(plog::debug) {
		LOG_DEBUG << message;
		return;
	}
	Logger::log(message);
}

} /* namespace logger */
