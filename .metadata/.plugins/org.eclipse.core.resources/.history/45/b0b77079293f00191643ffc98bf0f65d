#ifndef SRC_LOGGER_LOGGER_INFO_H_
#define SRC_LOGGER_LOGGER_INFO_H_

#include "logger.h"
#include "plog/include/plog/Log.h"

using namespace plog;

namespace client {

class LoggerInfo {
private:
	Severity severity = debug;
public:
	LoggerInfo() {}
	virtual ~LoggerInfo() {}
	void log();
};

} /* namespace client */

#endif /* SRC_LOGGER_LOGGER_INFO_H_ */
