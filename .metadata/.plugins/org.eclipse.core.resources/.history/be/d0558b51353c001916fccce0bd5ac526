#include "disconnect.h"
#include "../../client/client_engine.h"

namespace client {

ResponsePacket Disconnect::run(ITerminalLayer* terminal, ClientEngine client_engine) {
	client_engine.stop_flag = true;
	return terminal->disconnect();
}

} /* namespace client */
