#include "logger.h"
#include "logger_info.h"
#include "plog/include/plog/Log.h"

namespace logger {

void LoggerInfo::log(std::string message) {
	IF_LOG(plog::info) {
		LOG_INFO << message;
		return;
	}
	Logger::log(message);
}

} /* namespace logger */
